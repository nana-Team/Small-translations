			RuinSeekerConfigClient: {
				DisplayName: Ruin Seeker Client Config

				Headers: {
					Generation: "[i:4765] Generation"
					Visuals: "[i:5354] Visuals"
					ModCompatibility: "[i:1344] Mod Compatibility"
				}

				genArena: {
					Label: "[i:3905] Construct an Arena"
					Tooltip: Generates a boss arena near the center of the world
				}

				genBadlands: {
					Label: "[i:3271] Weather the Badlands"
					Tooltip: Generate a badlands microbiome in the desert
				}

				genDesertTree: {
					Label: "[i:3272] Grow a Desert Tree"
					Tooltip: Generate a bleached tree in the desert
				}

				genJungleFossilStone: {
					Label: "[i:RuinSeeker/Shellstone] Fossilize Jungle Stone"
					Tooltip: Generates gray painted shell piles and living mahogany inside jungle stone
				}

				genThinIceSpikes: {
					Label: "[i:4391] Generate Thin Ice Spikes"
					Tooltip: Generates small thin ice spikes on the ground in the snow biome
				}

				genOceanSeaweed: {
					Label: "[i:1112] Generate Seaweed in the Ocean"
					Tooltip:
						'''
						Generates seaweed inside the surface layer in the ocean 
						Consider disabling this for mods like Spirit which add their own ocean decoration 
						Leaving this on in such a case shouldn't cause issues regardless, this is only useful for aesthetic preferences
						'''
				}

				genMinecartSupportsOnWalls: {
					Label: "[i:RuinSeeker/Decorated_Vertical_Log_Wall] Generate Minecart Support Beams over walls"
					Tooltip: Minecart track support beams override any walls in their way
				}

				doMushroomSpores: {
					Label: "[i:RuinSeeker/Brown_Mushroom_Small] Mushrooms emit spore clouds"
					Tooltip: ""
				}

				deleteSLRLeafWalls: {
					Label: "[i:745] Remove Starlight River Leaf Walls"
					Tooltip:
						'''
						SLR leaf walls often improperly generate in strange places, like on top of forest structures 
						This setting will delete all SLR leaf walls on world generation
						'''
				}

				doMagmaLayerGlow: {
					Label: "[i:1322] Thermal Caves magma layer background glows"
					Tooltip: Enable this setting to enable faint lighting on all non-solid tiles without walls in the Thermal Caves magma layer.
